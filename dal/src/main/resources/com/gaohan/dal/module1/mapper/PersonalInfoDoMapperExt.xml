<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.gaohan.dal.module1.mapper.PersonalInfoDoMapperExt" >
    <select id="selectVoByPrimaryKey" resultType="com.gaohan.biz.module1.vo.PersonalInfoVo">
        select
        id as id,
        name as name,
        sex as sex,
        age as age,
        hobbies as hobbies
        from gaohan_personal.personal_info
        where id = #{id}
    </select>
</mapper>